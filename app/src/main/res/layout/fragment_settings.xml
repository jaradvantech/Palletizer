<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:context="com.example.administrator.Palletizer.Settings">

    <!-- TODO: Update blank fragment layout -->

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <com.bartoszlipinski.flippablestackview.FlippableStackView
            android:id="@+id/language_stack"
            android:layout_width="171dp"
            android:layout_height="0dp"
            android:layout_marginStart="128dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0">

        </com.bartoszlipinski.flippablestackview.FlippableStackView>

        <EditText
            android:id="@+id/editText_PC_IP_Address_1"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8px"
            android:layout_marginStart="160dp"
            android:layout_marginTop="8px"
            android:ems="10"
            android:imeOptions="actionNext"
            android:inputType="number"
            android:nextFocusForward="@+id/editText_PC_IP_Address_2"
            android:text="192"
            android:textAlignment="center"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/language_stack"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.417" />

        <EditText
            android:id="@+id/editText_PC_IP_Address_2"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            android:ems="10"
            android:imeOptions="actionNext"
            android:inputType="number"
            android:nextFocusForward="@+id/editText_PC_IP_Address_3"
            android:text="168"
            android:textAlignment="center"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="@+id/editText_PC_IP_Address_1"
            app:layout_constraintStart_toEndOf="@+id/textView3" />

        <EditText
            android:id="@+id/editText_PC_IP_Address_3"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            android:ems="10"
            android:imeOptions="actionNext"
            android:inputType="number"
            android:nextFocusForward="@+id/editText_PC_IP_Address_4"
            android:text="1"
            android:textAlignment="center"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="@+id/editText_PC_IP_Address_2"
            app:layout_constraintStart_toEndOf="@+id/textView4" />

        <EditText
            android:id="@+id/editText_PC_IP_Address_4"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            android:ems="10"
            android:imeOptions="actionNext"
            android:inputType="number"
            android:nextFocusForward="@+id/editText_PC_IP_Address_Port"
            android:text="100"
            android:textAlignment="center"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="@+id/editText_PC_IP_Address_3"
            app:layout_constraintStart_toEndOf="@+id/textView5" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginStart="6dp"
            android:text=":"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="@+id/editText_PC_IP_Address_4"
            app:layout_constraintStart_toEndOf="@+id/editText_PC_IP_Address_4" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            android:text="."
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="@+id/editText_PC_IP_Address_1"
            app:layout_constraintStart_toEndOf="@+id/editText_PC_IP_Address_1" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginStart="6dp"
            android:text="."
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="@+id/editText_PC_IP_Address_2"
            app:layout_constraintStart_toEndOf="@+id/editText_PC_IP_Address_2" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:layout_marginStart="6dp"
            android:text="."
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="@+id/editText_PC_IP_Address_3"
            app:layout_constraintStart_toEndOf="@+id/editText_PC_IP_Address_3" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/IPaddres"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="@+id/editText_PC_IP_Address_4"
            app:layout_constraintStart_toStartOf="@+id/editText_PC_IP_Address_1"
            app:layout_constraintTop_toBottomOf="@+id/editText_PC_IP_Address_3" />

        <EditText
            android:id="@+id/editText_PC_IP_Address_Port"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            android:ems="10"
            android:inputType="number"
            android:text="23623"
            android:textAlignment="center"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="@+id/editText_PC_IP_Address_4"
            app:layout_constraintStart_toEndOf="@+id/textView9" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/Port"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="@+id/editText_PC_IP_Address_Port"
            app:layout_constraintStart_toStartOf="@+id/editText_PC_IP_Address_Port"
            app:layout_constraintTop_toBottomOf="@+id/editText_PC_IP_Address_Port" />

        <ImageView
            android:id="@+id/settings_ImageView_save"
            android:layout_width="150dp"
            android:layout_height="150dp"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="160dp"
            android:layout_marginTop="8dp"
            android:src="@mipmap/save"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.436" />

        <TextView
            android:id="@+id/settings_TextView_credits"
            android:layout_width="wrap_content"
            android:layout_height="27dp"
            android:layout_marginBottom="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp"
            android:text="By José Andrés Grau Martínez and Roberto Salinas Rosich"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

    </android.support.constraint.ConstraintLayout>
</FrameLayout>
